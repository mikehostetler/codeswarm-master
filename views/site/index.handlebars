<script src="/js/controllers/codeswarmController.js"></script>

<script src="/js/controllers/user/loginController.js"></script>
<script src="/js/controllers/user/logoutController.js"></script>
<script src="/js/controllers/user/registerController.js"></script>
<script src="/js/controllers/user/accountController.js"></script>
<script src="/js/controllers/user/forgotpasswordController.js"></script>
<script src="/js/controllers/user/userindexController.js"></script>

<script src="/js/controllers/mainController.js"></script>

<script src="/js/controllers/home/indexController.js"></script>
<script src="/js/controllers/home/aboutController.js"></script>
<script src="/js/controllers/home/contributeController.js"></script>
<script src="/js/controllers/home/searchController.js"></script>
<script src="/js/controllers/home/supportController.js"></script>

<script src="/js/controllers/org/buildController.js"></script>
