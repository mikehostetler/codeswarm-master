<script src="/js/vendor/github/github.js"></script>
<script src="/js/vendor/gravatarjs/gravatar.js"></script>
<script src="/js/vendor/jquery/dist/jquery.js"></script>
<script src="/js/vendor/lodash/dist/lodash.js"></script>
